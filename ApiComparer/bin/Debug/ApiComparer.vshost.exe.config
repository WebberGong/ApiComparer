<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <add key="UrlContainsKeys" value="
         i.instagram.com
         www.instagram.com
         graph.facebook.com
         "/>
    <add key="SpecifiedUrlRegexes" value="
         https://www.instagram.com/(\w+)
         https://i.instagram.com/api/v1/channels/viewer/discover_videos/(\w+)
         https://graph.facebook.com/v2.3/(\w+)/activities
         https://i.instagram.com/api/v1/friendships/create/(\w+)/
         https://i.instagram.com/api/v1/friendships/show/(\w+)/
         https://i.instagram.com/api/v1/friendships/(\w+)/following/
         https://i.instagram.com/api/v1/media/(\w+)/like/
         https://i.instagram.com/api/v1/media/(\w+)/info/
         https://i.instagram.com/api/v1/users/(\w+)/info/
         https://i.instagram.com/api/v1/feed/user/(\w+)/story/
         https://i.instagram.com/api/v1/feed/user/(\w+)/(\?\S+)?
         "/>
  </appSettings>
</configuration>